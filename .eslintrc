{
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prop-types": 0,
    "react/display-name": 0,
    "react/no-children-prop": 0
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "env": {
    "node": true,
    "browser": true,
    "es6": true,
    "serviceworker": true
  }
}
